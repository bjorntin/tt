{
  "name": "images-gallery",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.2.0"
  },
  "scripts": {
    "start": "expo start",
    "prebuild": "npx expo prebuild",
    "ios:release": "BUILD_TYPE=release expo run:ios --configuration Release -d",
    "ios": "BUILD_TYPE=dev expo run:ios -d",
    "android:release": "BUILD_TYPE=release expo run:android --variant release -d",
    "android": "BUILD_TYPE=dev expo run:android -d",
    "prebuild:tv": "EXPO_TV=1 npx expo prebuild",
    "tvos:release": "BUILD_TYPE=release EXPO_TV=1 expo run:ios --configuration Release -d",
    "tvos": "BUILD_TYPE=dev EXPO_TV=1 expo run:ios -d",
    "androidtv:release": "BUILD_TYPE=release EXPO_TV=1 expo run:android --variant release -d",
    "androidtv": "BUILD_TYPE=dev EXPO_TV=1 expo run:android -d",
    "web:release": "BUILD_TYPE=release expo start --web -c",
    "web": "BUILD_TYPE=dev expo start --web -c",
    "lint": "expo lint",
    "tsc": "tsc"
  },
  "dependencies": {
    "@shopify/flash-list": "1.8.3",
    "expo": "53.0.15",
    "expo-constants": "^17.1.7",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.1",
    "expo-image": "~3.0.0",
    "expo-image-manipulator": "~13.1.6",
    "expo-linking": "^7.1.5",
    "expo-media-library": "~17.1.7",
    "expo-router": "^5.1.0",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "^2.2.3",
    "metro": "0.82.4",
    "p-limit": "^6.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "npm:react-native-tvos@0.79.5-0",
    "react-native-logs": "^5.3.0",
    "react-native-mmkv": "^3.3.0",
    "react-native-pixel-perfect": "^1.0.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.5.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-tvos/config-tv": "^0.1.3",
    "@types/cli-progress": "^3.11.6",
    "@types/react": "~19.0.10",
    "cli-progress": "^3.12.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "inquirer": "12.6.3",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    },
    "autolinking": {
      "android": {
        "buildFromSource": [
          "expo-image"
        ]
      }
    }
  }
}
